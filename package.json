{
  "name": "tech-triangle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android  --active-arch-only",
    "ios": "react-native run-ios",
    "lint": "biome lint",
    "eslint": "eslint . --fix",
    "format": "biome format --write",
    "dev": "react-native start",
    "test": "jest",
    "build": "cd android && gradlew assembleRelease && cd .. && npm run install-release-apk",
    "assembleRelease": "cd android && gradlew assembleRelease",
    "open": "cd android/app/build/outputs/apk/release/ && explorer .",
    "bundle": "cd android && gradlew bundleRelease",
    "clean": "cd android && gradlew clean",
    "install-apk": "adb install -r android/app/build/outputs/apk/debug/app-arm64-v8a-release.apk",
    "install-release-apk": "adb install -r android/app/build/outputs/apk/release/app-arm64-v8a-release.apk",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/bottom-tabs": "^7.3.14",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/stack": "^7.3.3",
    "@shopify/react-native-skia": "^2.0.1",
    "@tanstack/react-query": "^5.79.0",
    "clsx": "^2.1.1",
    "hono": "^4.7.10",
    "lottie-react-native": "^7.2.2",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-device-info": "^14.0.4",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "^15.12.0",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.42",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@babel/runtime": "^7.27.1",
    "@biomejs/biome": "1.9.4",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@tanstack/eslint-plugin-query": "^5.78.0",
    "@types/jest": "^29.5.14",
    "@types/react": "19.0.0",
    "@types/react-test-renderer": "19.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "^19.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  }
}